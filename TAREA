//ejercicio 65
funcion veces<- buscarcaracteres ( cadena, letra )
	Definir cont Como Entero
	para i<-1 hasta longitud(cadena) con paso 1
		cont=cont+1
	FinPara
	veces=cont
FinFuncion

funcion separarletras (palabra)
	Definir size,veces Como Entero
	size=longitud(palabra)
	para i<-1 hasta size con paso 1
		veces=buscarcaracteres (palabra,Subcadena(palabra,i,i))
		Escribir "La letra ",Subcadena(palabra,i,i)," esta", veces," veces en la palabra", palabra
	FinPara
FinFuncion


//Algoritmo contarcaractererRepetidos
Definir cadena Como Caracter
Escribir "Digite una palabra"
leer cadena
separarletras(cadena)

//FinAlgoritmo

