para x=1 Hasta 10 Con Paso 1 Hacer
	Escribir "Ingrese un numero "
	leer vector(x)
FinPara
Escribir "Ingresa el numero que desea buscar"
leer n
c=0
para x=1 Hasta 10 Con Paso 1 Hacer
	si n =vector(x) Entonces
		Escribir "El numero ",n," se encuentra en la poasicion ",x
		c=1
	FinSi
FinPara
si c=0 Entonces
	Escribir "EL numero ingresado no se encuentra en el vector"
FinSi

//ejercicio 55
Definir x,pares,impares,vector Como Entero
Dimension vector[10]
para x=1 Hasta 10 Con Paso 1 Hacer
	Escribir "Ingrese un numero"
	leer vector(x)
FinPara
pares=0
impares=0
para x=1 Hasta 10 Con Paso 1 Hacer
	si vector(x) mod 2=0 Entonces
		pares=pares+1
	SiNo
		impares=impares +1
	FinSi
FinPara
Escribir "Los numeors pares son: ",pares
Escribir "Los numeros impares son: ",impares

